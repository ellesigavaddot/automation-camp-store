  },
  "homepage": "https://github.com/ellesigavaddot/automation-camp-store#readme",
  "devDependencies": {
    "cypress": "^12.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "npm-run-all": "^4.1.5"
  }
}
